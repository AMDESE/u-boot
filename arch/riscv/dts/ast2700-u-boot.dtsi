/ {
	cpus {
		bootph-all;
	};

	memory@80000000 {
		bootph-all;
	};

	soc0: soc@12000000 {
		bootph-all;
		sdrammc: sdrammc@12c00000 {
			bootph-all;
		};
		pcie_ep: pcie_ep@12c15000 {
			bootph-all;
		};
	};

	soc1: soc@14000000 {
		bootph-all;
		uart12: serial@14c33b00 {
			bootph-all;
		};
	};

};
