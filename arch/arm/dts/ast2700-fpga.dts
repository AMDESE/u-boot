/dts-v1/;

#include "ast2700-u-boot.dtsi"

/ {
	memory {
		device_type = "memory";
		reg = <0x4 0x00000000 0x0 0x20000000>;
	};

	chosen {
		stdout-path = &uart5;
	};

	cpus {
		cpu@0 {
			clock-frequency = <50000000>;
		};
		cpu@1 {
			clock-frequency = <50000000>;
		};
		cpu@2 {
			clock-frequency = <50000000>;
		};
		cpu@3 {
			clock-frequency = <50000000>;
		};
	};
};

&uart5 {
	u-boot,dm-pre-reloc;
	status = "okay";
};

