/dts-v1/;

#include "ast2700-u-boot.dtsi"

/ {
	memory {
		device_type = "memory";
		reg = <0x4 0x00000000 0x0 0x20000000>;
	};

	chosen {
		stdout-path = &uart5;
	};

	aliases {
		ethernet0 = &mac0;
	};

	cpus {
		cpu@0 {
			clock-frequency = <50000000>;
		};
		cpu@1 {
			clock-frequency = <50000000>;
		};
		cpu@2 {
			clock-frequency = <50000000>;
		};
		cpu@3 {
			clock-frequency = <50000000>;
		};
	};
};

&uart5 {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&mdio0 {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;
	ethphy0: ethernet-phy@0 {
		reg = <0>;
	};
};

&mac0 {
	status = "okay";
	phy-mode = "rgmii";
	phy-handle = <&ethphy0>;
};
